    "pdf.js-extract": "^0.2.1",
    "pdf2json": "^3.1.4",
    "pdf2pic": "^3.1.3",
    "pdfjs-dist": "^4.6.82",
    "pdfreader": "^3.0.5",
    "pngjs": "^7.0.0",
    "qrcode-reader": "^1.0.4",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.33.5",
    "tesseract.js": "^5.1.1"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "overrides": {
    "strtok3": "8.0.1"
  },
  "resolutions": {
    "strtok3": "8.0.1"
  },
  "pnpm": {
    "overrides": {
      "strtok3": "8.0.1"
    }
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app"
  },
  "prettier": "@adonisjs/prettier-config"
